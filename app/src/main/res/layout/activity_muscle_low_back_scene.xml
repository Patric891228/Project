<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".scene.MuscleLowBackScene">
    <ImageView
        android:id="@+id/BG"
        android:background="@drawable/mm_lowback"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
    <ImageView
        android:id="@+id/BF1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/H0"
        app:layout_constraintBottom_toBottomOf="@id/H1"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V1"/>
    <ImageView
        android:id="@+id/BF2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/H1"
        app:layout_constraintBottom_toBottomOf="@id/H2"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V1"/>
    <ImageView
        android:id="@+id/BF3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/H2"
        app:layout_constraintBottom_toBottomOf="@id/H3"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V1"/>
    <ImageView
        android:id="@+id/BF4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/H3"
        app:layout_constraintBottom_toBottomOf="@id/H4"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V1"/>
    <ImageView
        android:id="@+id/BF5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/H4"
        app:layout_constraintBottom_toBottomOf="@id/H5"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V1"/>

    <ImageView
        android:id="@+id/BF6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/H6"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V1"
        app:layout_constraintTop_toTopOf="@id/H5"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/BF7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/H6"
        app:layout_constraintBottom_toBottomOf="@id/H7"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V2"/>
    <ImageView
        android:id="@+id/BF8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/H7"
        app:layout_constraintBottom_toBottomOf="@id/H8"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V2"/>
    <ImageView
        android:id="@+id/BF9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toTopOf="@id/H8"
        app:layout_constraintBottom_toBottomOf="@id/H9"
        app:layout_constraintLeft_toLeftOf="@id/V0"
        app:layout_constraintRight_toRightOf="@id/V2"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H0"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.19"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.26"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.34"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.41"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.49"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.56"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.64"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H7"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.72"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H8"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.79"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H9"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.87"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/H10"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.49"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/V0"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.31"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/V1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.64"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/V2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.82"/>
    <ImageView
        android:id="@+id/Back"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/magazine_muscle_back"
        app:layout_constraintTop_toTopOf="@id/Back_Top"
        app:layout_constraintBottom_toBottomOf="@id/Back_Bottom"
        app:layout_constraintLeft_toLeftOf="@id/Back_Left"
        app:layout_constraintRight_toRightOf="@id/Back_Right"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Back_Top"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Back_Bottom"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.13"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Back_Left"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.894"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Back_Right"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.96"/>
</androidx.constraintlayout.widget.ConstraintLayout>