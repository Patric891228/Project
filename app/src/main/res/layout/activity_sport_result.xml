<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".scene.SportResultScene">
    <ImageView
        android:id="@+id/BackGround"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/endexercise"/>
    <TextView
        android:id="@+id/Probaibility"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="0.0"
        android:textSize="22dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="@id/Probaibility_Top"
        app:layout_constraintBottom_toBottomOf="@id/Probaibility_Bottom"
        app:layout_constraintLeft_toLeftOf="@id/Probaibility_Left"
        app:layout_constraintRight_toRightOf="@id/Probaibility_Right"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Probaibility_Top"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.65"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Probaibility_Bottom"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.75"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Probaibility_Left"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Probaibility_Right"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.55"/>
    <ImageView
        android:id="@+id/BackHome"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/endexercise_button"
        app:layout_constraintTop_toTopOf="@id/BackHome_Top"
        app:layout_constraintBottom_toBottomOf="@id/BackHome_Bottom"
        app:layout_constraintLeft_toLeftOf="@id/BackHome_Left"
        app:layout_constraintRight_toRightOf="@id/BackHome_Right"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/BackHome_Top"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.7"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/BackHome_Bottom"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.97"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/BackHome_Left"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.82"/>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/BackHome_Right"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.96"/>
</androidx.constraintlayout.widget.ConstraintLayout>