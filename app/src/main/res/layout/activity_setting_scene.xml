    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/title_guideline_Top_for_Female"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.37" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/title_guideline_Right_for_Female"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.616" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/title_guideline_Bottom_for_Female"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.42" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/title_guideline_Left_for_Female"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.591" />
    <!--Female_Click-->
    <ImageView
        android:id="@+id/Female_Click"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/username_femaleselected"
        app:layout_constraintBottom_toBottomOf="@id/title_guideline_Bottom_for_Female"
        app:layout_constraintEnd_toEndOf="@id/title_guideline_Right_for_Female"
        app:layout_constraintStart_toStartOf="@id/title_guideline_Left_for_Female"
        app:layout_constraintTop_toTopOf="@id/title_guideline_Top_for_Female" />
    <!--Male-->
    <ImageView
        android:id="@+id/Male"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/account_inputbox_male"
        app:layout_constraintBottom_toBottomOf="@id/title_guideline_Bottom_for_Male"
        app:layout_constraintEnd_toEndOf="@id/title_guideline_Right_for_Male"
        app:layout_constraintStart_toStartOf="@id/title_guideline_Left_for_Male"
        app:layout_constraintTop_toTopOf="@id/title_guideline_Top_for_Male" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/title_guideline_Top_for_Male"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.37" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/title_guideline_Right_for_Male"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.545" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/title_guideline_Bottom_for_Male"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.42" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/title_guideline_Left_for_Male"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.52" />
    <!--Male_Click-->
    <ImageView
        android:id="@+id/Male_Click"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/username_femaleselected"
        app:layout_constraintBottom_toBottomOf="@id/title_guideline_Bottom_for_Male"
        app:layout_constraintEnd_toEndOf="@id/title_guideline_Right_for_Male"
        app:layout_constraintStart_toStartOf="@id/title_guideline_Left_for_Male"
        app:layout_constraintTop_toTopOf="@id/title_guideline_Top_for_Male" />
<!--Pen-->
    <ImageView
        android:id="@+id/Pen1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/username_editsaved"
        app:layout_constraintBottom_toBottomOf="@id/Bottom_for_Pen1"
        app:layout_constraintEnd_toEndOf="@id/Right_for_Pen1"
        app:layout_constraintStart_toStartOf="@id/Left_for_Pen1"
        app:layout_constraintTop_toTopOf="@id/Top_for_Pen1" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Top_for_Pen1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.37" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Right_for_Pen1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.69" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Bottom_for_Pen1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.42" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Left_for_Pen1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.66" />
    <ImageView
        android:id="@+id/Pen2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/username_editsaved"
        app:layout_constraintBottom_toBottomOf="@id/Bottom_for_Pen2"
        app:layout_constraintEnd_toEndOf="@id/Right_for_Pen2"
        app:layout_constraintStart_toStartOf="@id/Left_for_Pen2"
        app:layout_constraintTop_toTopOf="@id/Top_for_Pen2" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Top_for_Pen2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.515" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Right_for_Pen2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.716" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Bottom_for_Pen2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.56" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Left_for_Pen2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.691" />
    <ImageView
        android:id="@+id/Pen3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/username_editsaved"
        app:layout_constraintBottom_toBottomOf="@id/Bottom_for_Pen3"
        app:layout_constraintEnd_toEndOf="@id/Right_for_Pen3"
        app:layout_constraintStart_toStartOf="@id/Left_for_Pen3"
        app:layout_constraintTop_toTopOf="@id/Top_for_Pen3" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Top_for_Pen3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.655" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Right_for_Pen3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.86" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Bottom_for_Pen3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.701" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/Left_for_Pen3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.83" />

</androidx.constraintlayout.widget.ConstraintLayout>
