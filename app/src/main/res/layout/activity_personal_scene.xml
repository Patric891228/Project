<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".scene.PersonalScene">
<!--    GuildLine-->
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/top_of_setting_line"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.30"></androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/bottom_of_setting_line"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.70"></androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/right_of_setting_line"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.8"></androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/left_of_setting_line"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.2"></androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/middle_of_setting_line1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.3"></androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/middle_of_setting_line2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.4"></androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/middle_of_setting_line3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"></androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/middle_of_setting_line4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.6"></androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/middle_of_setting_line5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.7"></androidx.constraintlayout.widget.Guideline>
    <!--ImageButton-->
    <ImageButton
        android:id="@+id/toAccountSetScene"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/bottom_of_setting_line"
        app:layout_constraintTop_toTopOf="@id/top_of_setting_line"
        app:layout_constraintRight_toRightOf="@id/middle_of_setting_line1"
        app:layout_constraintLeft_toLeftOf="@id/left_of_setting_line"
        tools:srcCompat="@tools:sample/avatars" />
    <ImageButton
        android:id="@+id/toRemind"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/bottom_of_setting_line"
        app:layout_constraintTop_toTopOf="@id/top_of_setting_line"
        app:layout_constraintRight_toRightOf="@id/middle_of_setting_line2"
        app:layout_constraintLeft_toLeftOf="@id/middle_of_setting_line1"
        tools:srcCompat="@tools:sample/avatars" />
    <ImageButton
        android:id="@+id/toBGM"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/bottom_of_setting_line"
        app:layout_constraintTop_toTopOf="@id/top_of_setting_line"
        app:layout_constraintRight_toRightOf="@id/middle_of_setting_line3"
        app:layout_constraintLeft_toLeftOf="@id/middle_of_setting_line2"
        tools:srcCompat="@tools:sample/avatars" />
    <ImageButton
        android:id="@+id/Back_From_Personal_Scene"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/bottom_of_setting_line"
        app:layout_constraintTop_toTopOf="@id/top_of_setting_line"
        app:layout_constraintRight_toRightOf="@id/middle_of_setting_line4"
        app:layout_constraintLeft_toLeftOf="@id/middle_of_setting_line3"
        tools:srcCompat="@tools:sample/avatars" />
    <ImageButton
        android:id="@+id/toSetting"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="@id/bottom_of_setting_line"
        app:layout_constraintTop_toTopOf="@id/top_of_setting_line"
        app:layout_constraintRight_toRightOf="@id/middle_of_setting_line5"
        app:layout_constraintLeft_toLeftOf="@id/middle_of_setting_line4"
        tools:srcCompat="@tools:sample/avatars" />

    <Button
        android:id="@+id/LogOutButton"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="LogOut"
        app:layout_constraintBottom_toBottomOf="@id/bottom_of_setting_line"
        app:layout_constraintLeft_toLeftOf="@id/middle_of_setting_line5"
        app:layout_constraintRight_toRightOf="@id/right_of_setting_line"
        app:layout_constraintTop_toTopOf="@id/top_of_setting_line" />

</androidx.constraintlayout.widget.ConstraintLayout>